{
    "manifest_version": 3,
    "name": "Colocar Lembrete",
    "version": "1.0",
    "description": "Extens√£o para adicionar lembretes automaticamente.",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["*://*/*"],
    "background": {
        "service_worker": "src/background.js"
    },
    "action": {
        "default_title": "Adicionar Lembrete",
        "default_icon": "icon.png",
        "default_popup": "src/popup.html"
    }
}